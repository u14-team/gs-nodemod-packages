{
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "esModuleInterop": true,
    "types": ["node", "@gs-nodemod/types"],
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSourceMap": true,
    "resolveJsonModule": true,
    "declaration": true,
  },
}